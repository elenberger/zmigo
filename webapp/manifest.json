{
	"_version": "1.5.0",
	"sap.app": {
		"id": "zteliaZMIGO",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "/sap/opu/odata/sap/ZLO_MATDOC_CREATE_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion":  "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		},
		"sourceTemplate": {
			"id": "ui5template.2masterdetailcrud",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://detail-view",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "ztelia.view.App",
			"type": "XML",
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ztelia.i18n.i18n"
				}
			},
			"": {
				"dataSource": "mainService",
				"settings": {
					"metadataUrlParams": {
						"sap-documentation": "heading"
					},
					"defaultBindingMode": "TwoWay"
				}
			},
			"local": {
				"type": "sap.ui.model.json.JSONModel"
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "ztelia.view",
				"controlId": "idAppControl",
				"controlAggregation": "detailPages",
				"bypassed": {
					"target": [
						"notFound"
					]
				},
				"async": "true"
			},
			"routes": [
				{
					"pattern": "",
					"name": "initial",
					"target": [
						"scenario"
					]
				},
				{
					"pattern": "Scenario/{SC}",
					"name": "items",
					"target": [
						"master",
						"items"
					]
				},
				{
					"pattern": "Scenario/{SC}/{Matnr}/:Equnr:/{Werks}/{Lgort}/{Charg}",
					"name": "object",
					"target": [
						"master",
						"object"
					]
				},
				{
					"pattern": "Scenario/{SC}/{db_key}",
					"name": "process",
					"target": [
						"process" ]
				}
				
			],
			"targets": {
				"master": {
					"viewName": "Master",
					"viewLevel": 1,
					"viewId": "master",
					"controlAggregation": "masterPages"
				},
				"scenario": {
					"viewName": "Scenario",
					"viewId": "scenario",
					"viewLevel": 2
				},
				"items": {
					"viewName": "Detail",
					"viewId": "detail",
					"viewLevel": 2
				},
				"object": {
					"viewName": "Object",
					"viewLevel": 2
				},
				"process": {
					"viewName": "CreateEntity",
					"viewLevel": 2
				},
				"detailObjectNotFound": {
					"viewName": "DetailObjectNotFound",
					"viewId": "detailObjectNotFound"
				},
				"detailNoObjectsAvailable": {
					"viewName": "DetailNoObjectsAvailable",
					"viewId": "detailNoObjectsAvailable"
				},
				"notFound": {
					"viewName": "NotFound",
					"viewId": "notFound"
				}
			}
		}
	}
}