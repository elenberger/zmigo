<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="ztelia.controller.Scenario"
	xmlns:html="http://www.w3.org/1999/xhtml" xmlns:semantic="sap.m.semantic">
	<semantic:DetailPage title="Choose scenario">
		<semantic:content>
			<html:br/>
			<html:br/>
			<HBox justifyContent="Center" >
				<List width="20EM" id="idList" items="{/ScenarioSet}">
					<ActionListItem text="{Description} " press="onSelect"></ActionListItem>
				</List>
			
			</HBox>
			</semantic:content>
	</semantic:DetailPage>
</mvc:View>